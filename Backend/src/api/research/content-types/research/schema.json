{
  "kind": "collectionType",
  "collectionName": "all_research",
  "info": {
    "singularName": "research",
    "pluralName": "all-research",
    "displayName": "Research",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "required": true,
      "minLength": 3,
      "targetField": "title"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "journal",
        "proceeding",
        "senior_projects"
      ],
      "required": true
    },
    "published_date": {
      "type": "date",
      "required": true
    },
    "research_database_url": {
      "type": "string"
    },
    "writer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::member.member"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    }
  }
}
